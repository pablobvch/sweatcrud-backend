{
  "name": "sweatcrud-backend",
  "version": "1.0.0",
  "description": "this is a nodejs backend",
  "main": "index.mjs",
  "scripts": {
    "build:dev": "flow-remove-types src/ -d build/",
    "build": "babel ./src --out-dir ./dist",
    "flow:check": "flow check ./src/",
    "flow:deps": "flow-typed install",
    "flow": "flow",
    "prestart": "yarn build",
    "start": "node --experimental-modules dist/index.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-watch"
  },
  "watch": {
    "build:dev": "src/**/*.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablobvch/sweatcrud-backend.git"
  },
  "keywords": [
    "expressjs",
    "nodejs",
    "postgresql",
    "knex"
  ],
  "author": "Pablo Bilevich Alaniz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pablobvch/sweatcrud-backend/issues"
  },
  "homepage": "https://github.com/pablobvch/sweatcrud-backend#readme",
  "dependencies": {
    "body-parser": "1.19.0",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "knex": "0.19.5",
    "momentjs": "2.0.0",
    "pg": "7.11.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/preset-flow": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-inline-dotenv": "1.3.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-rename-import": "2.3.0",
    "debug": "4.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "flow-bin": "0.95.1",
    "flow-remove-types": "2.102.0-rc",
    "flow-typed": "2.5.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "nodemon": "1.18.9",
    "npm-watch": "^0.6.0",
    "pm2": "3.5.0",
    "pm2-windows-startup": "1.0.3",
    "prettier": "1.15.3"
  },
  "lint-staged": {}
}
